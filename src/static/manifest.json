{
  "manifest_version": 3,
  "name": "React Chrome Extension BoilerPlate",
  "description": "A basic React chrome extension boilerplate made with TypeScript, webpack etc",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "React Extension",
    "default_icon": "icon.png"
  },
  "options_page": "options.html"
}
